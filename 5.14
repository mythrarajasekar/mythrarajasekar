def max_min_product(arr1, arr2):
    max_elem_arr1 = max(arr1)
    min_elem_arr2 = min(arr2)
    return max_elem_arr1 * min_elem_arr2

def main():
    arr1 = [int(x) for x in input("Enter the elements of the first array separated by spaces: ").split()]
    arr2 = [int(x) for x in input("Enter the elements of the second array separated by spaces: ").split()]

    result = max_min_product(arr1, arr2)
    print("Product of the maximum element in the first array and the minimum element in the second array:", result)

if __name__ == "__main__":
    main()
