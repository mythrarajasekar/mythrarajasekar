n = int(input("Enter how many values do you want to read: "))
sum_values = 0

# Read n values and compute their sum
count = 0
print(f"Enter {n} values: ", end="")
while count < n:
    value = int(input())
    sum_values += value
    count += 1

# Output the result
print("Sum of given values:", sum_values)
