def array_sum(arr):
    total_sum = 0
    for num in arr:
        total_sum += num
    return total_sum

def main():
    size = int(input("Enter how many values you want to read: "))
    arr = []
    
    for i in range(size):
        arr.append(int(input(f"Enter the value of a[{i}]: ")))
    
    sum_of_array = array_sum(arr)
    print("Sum of elements of the array:", sum_of_array)

if __name__ == "__main__":
    main()
